{
  "name": "bullmq-ai-agent",
  "version": "0.2.5",
  "description": "AI agent library using BullMQ Flows for parallel multi-agent orchestration with LangChain",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build",
    "example:single": "tsx examples/single-agent.ts",
    "example:multi": "tsx examples/multi-agent.ts",
    "example:rag": "tsx examples/rag-agent.ts"
  },
  "keywords": [
    "bullmq",
    "ai-agent",
    "langchain",
    "llm",
    "multi-agent",
    "orchestrator",
    "openai",
    "redis",
    "queue",
    "workflow"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/humbertogontijo/bullmq-ai-agent.git"
  },
  "author": "humbertogontijo",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@langchain/classic": "^1.0.21",
    "@langchain/community": "^1.1.20",
    "@langchain/cohere": "^1.0.4",
    "@langchain/core": "^1.1.29",
    "@langchain/openai": "^1.2.11",
    "@langchain/redis": "^1.1.0",
    "@langchain/textsplitters": "^1.0.1",
    "@sinclair/typebox": "^0.34.9",
    "bullmq": "^5.70.1",
    "cheerio": "^1.2.0",
    "langchain": "^1.2.28",
    "redis": "^4.6.13"
  },
  "devDependencies": {
    "@clack/prompts": "^1.0.1",
    "@types/node": "^25.3.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "ioredis": "^5.10.0"
  },
  "peerDependenciesMeta": {
    "ioredis": {
      "optional": true
    },
    "@langchain/anthropic": {
      "optional": true
    },
    "@langchain/google-genai": {
      "optional": true
    },
    "@langchain/cohere": {
      "optional": true
    }
  }
}
